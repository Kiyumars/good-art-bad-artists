<div class="container">
<h1 class='text-center'><%= @crime %></h1>
<div class="row">
	<% @artists.shuffle.each do |artist| %>
		<%= form_tag game_crime_path, method: "patch" do %>
	  	<%= hidden_field_tag(:artist_id, artist.id) %>
			<input type="image" class= 'col-md-4' src="<%= artist.image %>" name='<%= artist.id %>' width=250>
		<%end%>
	<%end%>
</div>
</div>